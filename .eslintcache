ost}/> : <EditTenPost edit={false} postInfo={blankTen} goalId ={goalId} setPostInfo={setTenPost}/> }\r\n\r\n    </>)\r\n}\r\n}\r\n\r\n\r\nexport default TenSummary;","C:\\Users\\ngail\\Desktop\\100days-front\\src\\TenMetrics.js",[],"C:\\Users\\ngail\\Desktop\\100days-front\\src\\helpers\\transformMetrics.js",[],"C:\\Users\\ngail\\Desktop\\100days-front\\src\\LineGraph.js",[],{"ruleId":"162","replacedBy":"163"},{"ruleId":"164","replacedBy":"165"},{"ruleId":"166","severity":1,"message":"167","line":1,"column":8,"nodeType":"168","messageId":"169","endLine":1,"endColumn":13},{"ruleId":"166","severity":1,"message":"170","line":14,"column":10,"nodeType":"168","messageId":"169","endLine":14,"endColumn":16},{"ruleId":"166","severity":1,"message":"171","line":15,"column":10,"nodeType":"168","messageId":"169","endLine":15,"endColumn":18},{"ruleId":"172","severity":1,"message":"173","line":43,"column":5,"nodeType":"174","endLine":43,"endColumn":26,"suggestions":"175"},{"ruleId":"166","severity":1,"message":"176","line":30,"column":12,"nodeType":"168","messageId":"169","endLine":30,"endColumn":16},{"ruleId":"166","severity":1,"message":"177","line":30,"column":18,"nodeType":"168","messageId":"169","endLine":30,"endColumn":25},{"ruleId":"178","severity":1,"message":"179","line":75,"column":1,"nodeType":"180","messageId":"181","endLine":79,"endColumn":15},{"ruleId":"172","severity":1,"message":"182","line":38,"column":7,"nodeType":"174","endLine":38,"endColumn":20,"suggestions":"183"},"no-native-reassign",["184"],"no-negated-in-lhs",["185"],"no-unused-vars","'React' is defined but never used.","Identifier","unusedVar","'goalId' is assigned a value but never used.","'startDay' is assigned a value but never used.","react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'setGoalId' and 'setStartDay'. Either include them or remove the dependency array.","ArrayExpression",["186"],"'isAm' is assigned a value but never used.","'setIsAm' is assigned a value but neve